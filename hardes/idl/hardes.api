import (
	"get_conf/get_conf.api"
	"save/save.api"
)

@server(
	jwt:			Auth
	middleware: 	AuthMiddleware
	prefix:			api
	group:			conf
)

service hardes-api {
	@handler save
	post /hardes/v1/conf/:id (SaveReq) returns (SaveResp)
	
	@handler getConf
	get /hardes/v1/conf/:id (GetConfReq) returns (GetConfResp)
}